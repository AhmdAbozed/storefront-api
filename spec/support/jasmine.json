{
  "spec_dir": "dist/tests",
  "spec_files": [
    "**/*[sS]pec.?(m)js","dist/tests/*[sS]pec.?(m)js","dist/tests/*[sS]pec.js"
  ],
  "helpers": [
    "helpers/**/*.js","../helpers/*.js"
  ],
  "reporters": [
    {
      "name": "jasmine-spec-reporter#SpecReporter",
      "options": {
        "displayStacktrace": "all"
      }
    }
  ],
  "stopSpecOnExpectationFailure": false,
  "random": false
}
