{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "type": "module",
  "scripts": {
    "start": "npx ts-node src/server.ts",
    "DB": "db-migrate --env placeholder db:create store_front_db",
    "testDB": "db-migrate --env placeholder db:create store_test_db",
    "migrateUp": "cross-env ENV=dev db-migrate --env dev up",
    "migrateDown": "cross-env ENV=dev db-migrate --env dev down",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "test": "jasmine-ts",
    "tsc": "tsc"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.0",
    "cross-env": "^7.0.3",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.1",
    "esm": "^3.2.25",
    "express": "^4.18.1",
    "npm-check-updates": "^16.0.1",
    "pg": "^8.7.3",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jasmine": "^4.0.3",
    "@types/pg": "^8.6.5",
    "jasmine": "^4.3.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3"
  }
}
