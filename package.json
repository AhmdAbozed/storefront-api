{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "cross-env ENV=dev npx ts-node src/server.ts",
    "DB": "db-migrate --env placeholder db:create store_front_db",
    "testDB": "db-migrate --env placeholder db:create store_test_db",
    "migrateUp": "cross-env ENV=dev db-migrate --env dev up",
    "migrateDown": "cross-env ENV=dev db-migrate --env dev down",
    "migrateUpTest": "cross-env ENV=test db-migrate --env test up",
    "migrateDownTest": "cross-env ENV=test db-migrate --env test down",
    "test": "tsc && jasmine",
    "migrateTest": "npm run migrateUpTest && (npm run test || npm run migrateDownTest) && npm run migrateDownTest"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/supertest": "^2.0.12",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.1",
    "esm": "^3.2.25",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "npm-check-updates": "^16.0.1",
    "pg": "^8.7.3",
    "supertest": "^6.2.4",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jasmine": "^4.0.3",
    "@types/pg": "^8.6.5",
    "jasmine": "^4.3.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3"
  }
}
